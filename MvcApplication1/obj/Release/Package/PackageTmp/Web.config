    <!--研究室サーバにアップロードする場合-->
    <add name="DefaultConnection" connectionString="$(ReplacableToken_DefaultConnection-Web.config Connection String_0)" providerName="System.Data.SqlClient" />
    <!--<add name="ApplicationServices" connectionString="Data Source=192.168.32.115,1433;Persist Security Info=True;User ID=Sample01R_Agent;Password=tcupo212;Database=Sample01R" providerName="System.Data.SqlClient" />-->
